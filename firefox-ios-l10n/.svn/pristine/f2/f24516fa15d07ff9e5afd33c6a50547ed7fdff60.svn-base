        <source>Create an account</source>
        <target>أنشئ حسابًا</target>
        <note>See http://mzl.la/1Qtkf0j</note>
      </trans-unit>
      <trans-unit id="Dark">
        <source>Dark</source>
        <target>غامق</target>
        <note>Dark theme setting in Reading View settings</note>
      </trans-unit>
