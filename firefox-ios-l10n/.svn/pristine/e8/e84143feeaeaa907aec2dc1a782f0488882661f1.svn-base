        <note>See http://mzl.la/1T8gxwo</note>
      </trans-unit>
      <trans-unit id="Choose">
        <source>Choose</source>
        <target>Velja</target>
        <note>See http://mzl.la/1if9ODp</note>
      </trans-unit>
