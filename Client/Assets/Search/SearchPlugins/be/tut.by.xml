<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
<ShortName>TUT.BY</ShortName>
<Description>Пошук TUT.BY</Description>
<InputEncoding>UTF-8</InputEncoding>
<Image width="16" height="16">data:image/png;base64,</Image>
<Url type="text/html" method="GET" template="http://search.tut.by/?">
  <Param name="query" value="{searchTerms}"/>
  <Param name="status" value="1"/>
  <Param name="encoding" value="1"/>
  <Param name="page" value="0"/>
  <Param name="how" value="rlv"/>
</Url>
<SearchForm>http://search.tut.by/</SearchForm>
</SearchPlugin>
